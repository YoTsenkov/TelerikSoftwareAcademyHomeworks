using System;
class SelectionSort
{
    static void Main()
    {
        int[] array = { 1, 2, 5, 3, 8, 4 };
        int min;
        int buff;

        for (int i = 0; i < array.Length; i++)
        {
            min = i;
            for (int j = i + 1; j < array.Length; j++)
            {
                if (array[j] < array[min])
                {
                    min = j;
                }
            }
            buff = array[i];
            array[i] = array[min];
            array[min] = buff;
        }
        for (int i = 0; i < array.Length; i++)
        {
            Console.WriteLine(array[i]);
        }
    }
}

